#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules/@crowdin/cli/jdeploy-bundle/node_modules:/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules/@crowdin/cli/node_modules:/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules/@crowdin/node_modules:/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules:/home/nick/alist-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules/@crowdin/cli/jdeploy-bundle/node_modules:/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules/@crowdin/cli/node_modules:/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules/@crowdin/node_modules:/home/nick/alist-web/node_modules/.pnpm/@crowdin+cli@3.7.10/node_modules:/home/nick/alist-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@crowdin/cli/jdeploy-bundle/jdeploy.js" "$@"
else
  exec node  "$basedir/../@crowdin/cli/jdeploy-bundle/jdeploy.js" "$@"
fi
